<h3 class="center-block">Displaying Users with lastName : {{searchTerm}}</h3>

<div class="center-block">
    <div>
        <table>
            <tr>
                <th>Id</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>

            {{#each employeeData}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.firstName}}</td>
                <td>{{this.lastName}}</td>
            </tr>
            {{/each}}

        </table>

    </div>
</div>